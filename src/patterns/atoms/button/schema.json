{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "/atom/button",
	"title": "button",
	"description": "A button atom",
	"type": "object",
	"properties": {
		"disabled": {
			"type": "boolean"
		},
		"children": { "type": "string" },
		"text": { "type": "string" }
	},
	"anyOf": [{ "required": ["children"] }, { "required": ["text"] }]
}
